char *read_map()
{
    char    *tm;
    char *p;
	int	fd;
    int i; 

    i = 1;
    tm = NULL;
    p = ft_strdup("");
	fd = open("map.ber", O_RDONLY);
    tm = get_next_line(fd);
    while(tm)
	{
        p = ft_strjoin(p, tm);
		tm = get_next_line(fd);
        i++;
	}
    
    printf("%s", p);
    return (p);
}